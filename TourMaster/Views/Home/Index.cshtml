    <!-- Most Popular Section -->
    <section id="most-popular">
        <!-- Section Heading -->
        <div class="section-heading-wrapper text-center">
            <h1 class="section-title text-uppercase">Most Popular Tours</h1>
            <h4 class="section-subtitle text-uppercase">
                Here is the
                <span>best selling</span> tours
            </h4>
        </div>
        <!-- Section Body -->
        <div class="section-body-wrapper">
            @{
                int Nr = 0;
                foreach (Tour tour in Model.MostPopular)
                {
                    string TourTitle = "";
                    if (tour.FromId == tour.DestinationId)
                    {
                        TourTitle = tour.City.CityName;
                    }
                    else
                    {
                        TourTitle = tour.City.CityName + " - " + tour.City1.CityName;
                    }

                    <!-- Section Item Left-Angle Start -->
                    <div class="best-tour clearfix">
                        <div class="tour-background" style="background-image: url('@tour.TourImage.ImageURL')">
                            <div class="@(Nr==1?"tour-info-right":"tour-info-left")"></div>
                        </div>
                        <div class="tour-info @(Nr==1?"right":"left")">
                            <div class="container">
                                <h1 class="tour-title text-uppercase">@TourTitle Tour</h1>
                                <h3 class="tour-subtitle text-capitalize">@tour.Duration @tour.DurationType.Type trip from @tour.City.CityName </h3>
                                <p class="tour-duration" title="Duration">
                                    <i class="fal fa-clock"></i>@tour.Duration @tour.DurationType.Type
                                </p>
                                <p class="tour-price" title="Price">
                                    <i class="fal fa-money-bill"></i>@tour.Price.ToString("#.##") @tour.Currency.CurrencyName
                                </p>
                                <button class="btn btn-secondary text-uppercase goto-tour-details" data-tour-id="@tour.Id">Details</button>
                            </div>
                        </div>
                    </div>
                    <!-- Section Item Left-Angle End -->
                    Nr++;
                }
            }
        </div>
    </section>

    <!-- Best Guides Section -->
