@using TourMaster.Models;
@model TourMaster.Models.ViewModel

<!DOCTYPE html>


<html lang="en">



<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewBag.Origin.Title</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,500,600,700,800,900;subset=latin-ext">
    <link rel="stylesheet" href="~/public/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.css">
    <link rel="stylesheet" href="~/public/css/default.css">
    <link rel="stylesheet" href="~/public/css/default.date.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/7.21.1/sweetalert2.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-bar-rating/1.2.2/themes/css-stars.min.css">
    <link rel="stylesheet" href="~/public/css/jalendar.css">
    <link rel="stylesheet" href="~/public/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/public/css/style.css">
</head>



<body data-spy="scroll" data-target="#navigation" data-offset="100">
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg" id="navigation">
        <div class="container">
            <!-- NavBar Logo Start -->
            <a href="@Url.Action("index", "home")" class="navbar-brand">
                <img src="@ViewBag.Origin.Logo" width="50%">
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-collapsable">
                <i class="fas fa-bars text-white"></i>
            </button>
            <!-- NavBar Logo End -->
            <!-- NavBar Menu Start -->
            <div class="collapse navbar-collapse" id="navbar-collapsable">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="#main-car">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="tours-modal-trigger" href="">Tours</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#most-popular">Most Popular</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#best-guides">Top Guides</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#about">About Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#contact">Contact</a>
                    </li>
                    @{
                        if (Session["User"] != null)
                        {
                            User logedUser = Session["user"] as User;
                            string userinitials = logedUser.Fullname.Split(' ')[0] + " " + logedUser.Fullname.Split(' ')[1][0] + ".";
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" id="notifications-dropdown" role="button" data-toggle="dropdown">
                                    <i class="fas fa-bell @(logedUser.Notifications.Count != 0?"text-danger":"")"></i><span class="badge @(logedUser.Notifications.Count !=0?"badge-danger":"badge-secondary")">@(logedUser.Notifications.Count != 0 ? logedUser.Notifications.Count : 0)</span>
                                </a>
                                <div class="dropdown-menu">
                                    @{
                                        if (logedUser.Notifications.Count != 0)
                                        {
                                            foreach (Notification noti in logedUser.Notifications.OrderByDescending(n => n.Date).Take(10))
                                            {
                                                <a class="dropdown-item" data-noti-id="@noti.Id">
                                                    <i class="@noti.NotificationType.Icon text-@noti.NotificationType.Color d-inline-block mr-2"></i>
                                                    <p class="noti-text">@noti.Text</p>
                                                </a>
                                            }
                                        }
                                    }
                                    <a href="#" class="dropdown-item" data-user-id="@logedUser.Id">See all notifications</a>
                                </div>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" id="profile-dropdown" role="button" data-toggle="dropdown">
                                    <img class="rounded-circle" src="@logedUser.ProfileImage" />
                                    <p class="user-initials ml-2">@userinitials</p>
                                </a>
                                <div class="dropdown-menu">
                                    @if (logedUser.AccountType == 1)
                                    {
                                        <a class="dropdown-item" href="@Url.Action("index", new { controller="home", area="manage" })">Dashboard</a>
                                    }
                                    <a class="dropdown-item" href="#">Notifications</a>
                                    <a class="dropdown-item" href="#">Messages</a>
                                    <a class="dropdown-item" href="#">Settings</a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item" href="@Url.Action("logout", "home")">Log Out</a>
                                </div>
                            </li>

                        }
                        else
                        {
                            <li class="nav-item sign-in">
                                <a class="nav-link text-center" id="signup-form-modal-trigger" href="">
                                    <i class="fas fa-user-plus"></i>
                                </a>
                            </li>
                            <li class="nav-item sign-in">
                                <a class="nav-link" id="login-form-modal-trigger" href="">Log In</a>
                            </li>
                        }
                    }
                </ul>
            </div>
            <!-- NavBar Menu End -->
        </div>
    </nav>

    <!-- SignUp Form Modal -->
    <div class="signup-form-modal-wrapper">
        <div class="modal" id="signup-form-modal" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-body" id="signup-form-body">
                        <div class="signup-form-heading text-center">
                            <img class="p-4" src="~/Public/images/logo/logo_orange.svg" width="40%"/>
                            <h1 class="text-uppercase text-center">welcome</h1>
                            <p class="text-uppercase text-center">
                                Fill the form and start
                                <span>touring</span>
                            </p>
                        </div>
                        <form action="@Url.Action("signup", "home")" method="post" id="signup-form" novalidate>
                            <div class="row">
                                <div class="col-md-6 mt-4">
                                    <div class="form-group">
                                        <label for="name">Name</label>
                                        <input class="form-control" type="text" name="name" id="name" data-validation="required length" data-validation-length="min3">
                                    </div>
                                    <div class="form-group">
                                        <label for="email">E-mail</label>
                                        <input class="form-control" type="email" name="email" id="email" data-validation="required email">
                                    </div>
                                    <div class="form-group">
                                        <label for="password">Password</label>
                                        <input class="form-control" type="password" name="password" id="password" data-validation="required strength length" data-validation-length="min8"
                                               data-validation-strength="2">
                                    </div>
                                    <div class="form-group">
                                        <label for="password-confirmation">Password Confirmation</label>
                                        <input class="form-control" type="password" name="passwordconfirmation" id="password-confirmation" data-validation="required confirmation"
                                               data-validation-confirm="password">
                                    </div>
                                    <div class="form-group">
                                        <label for="phone">Phone</label>
                                        <input class="form-control" type="text" name="phone" id="phone" data-validation="required number" data-validation-allowing="integer, positive">
                                    </div>
                                </div>
                                <div class="col-md-6 mt-4">
                                    <div class="form-group">
                                        <label for="surname">Surname</label>
                                        <input class="form-control" type="text" name="surname" id="surnname" data-validation="required length" data-validation-length="min3">
                                    </div>
                                    <div class="form-group">
                                        <label for="b-day-date">Birthday</label>
                                        <input class="datepicker form-control" name="birthday" id="b-day-date" data-validation="required">
                                    </div>
                                    <div class="form-group">
                                        <label for="country">Country</label>
                                        <select class="form-control" name="countryid" id="country" data-validation="required">
                                            <option value="" disabled selected></option>
                                            @foreach (Country cntr in ViewBag.Countries)
                                            {
                                                <option value="@cntr.Id">@cntr.CountryName</option>
                                            }
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label for="city">City</label>
                                        <select class="form-control" name="cityid" id="city" data-validation="required">
                                            <option value="" disabled selected></option>
                                        </select>
                                    </div>
                                    <div class="form-group mb-0 mt-4">
                                        <div class="form-check">
                                            <input class="form-check-input" type="radio" name="accounttypeid" id="account-type-traveler" value="0" checked>
                                            <label class="form-check-label" for="account-type-traveler">
                                                I want to
                                                <span>book</span> a tour.
                                            </label>
                                        </div>
                                        <div class="form-check">
                                            <input class="form-check-input" type="radio" name="accounttypeid" id="account-type-guide" value="1">
                                            <label class="form-check-label" for="account-type-guide">
                                                I am a
                                                <span>guide</span>.
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-buttons my-4">
                                <button type="submit" class="btn btn-success btn-block text-uppercase">Sign Up</button>
                                <button type="button" class="btn btn-outline-secondary btn-block" id="go-to-login">Already have an account? Log In</button>
                            </div>
                        </form>
                    </div>
                    <i class="fas fa-times" id="form-modal-close" data-dismiss="modal"></i>
                </div>
            </div>
        </div>
        @if (Session["SignedUpSuccess"] != null)
        {
            <span id="signup-alert-success" hidden></span>
            Session["SignedUpSuccess"] = null;
        }
        else if (Session["SignedUpError"] != null)
        {
            <span id="signup-alert-error" data-msg="@Session["SignUpMsg"]" hidden></span>
            Session["SignedUpError"] = null;
            Session["SignUpMsg"] = null;
        }
    </div>

    <!-- Login Form Modal -->
    <div class="login-form-modal-wrapper">
        <div class="modal" id="login-form-modal" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <!-- Tabs -->
                        <ul class="nav nav-tabs nav-justified" id="login-tabs" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active text-uppercase" href="#traveler-login" data-toggle="tab">Traveler Login</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-uppercase" href="#guide-login" data-toggle="tab">Guide Login</a>
                            </li>
                        </ul>
                        <!-- Tab Contents -->
                        <div class="tab-content">
                            <!-- Traveler Login Tab Start -->
                            <div class="tab-pane fade show active" id="traveler-login" role="tabpanel" aria-labelledby="home-tab">
                                <div class="login-form-heading text-center">
                                    <img class="p-4" src="~/Public/images/logo/logo_orange.svg" width="60%" />
                                    <h1 class="text-uppercase text-center">welcome</h1>
                                    <p class="text-uppercase text-center">
                                        if you are a
                                        <span>guide</span> use
                                        <t>guide login</t>
                                    </p>
                                </div>
                                <form action="@Url.Action("login", "home")" method="post" id="traveler-login-form" novalidate>
                                    <input type="number" name="accounttypeid" value="0" hidden />
                                    <div class="form-group">
                                        <label for="traveler-email">E-mail</label>
                                        <input class="form-control" type="email" name="email" id="traveler-email" data-validation="required email">
                                    </div>
                                    <div class="form-group">
                                        <label for="traveler-password">Password</label>
                                        <input class="form-control" type="password" name="password" id="traveler-password" data-validation="length" data-validation-length="min8">
                                    </div>
                                    <div class="form-buttons my-4 text-center">
                                        <button type="submit" class="btn btn-success btn-block text-uppercase">Log in</button>
                                        <button type="button" class="btn btn-outline-secondary btn-block go-to-signup">Don't have an account? Sign Up</button>
                                        <a href="#" class="btn btn-link pass-recovery">Forgot Password?</a>
                                    </div>
                                </form>
                            </div>
                            <!-- Traveler Login Tab End -->
                            <!-- Guide Login Tab Start -->
                            <div class="tab-pane fade" id="guide-login" role="tabpanel" aria-labelledby="profile-tab">
                                <div class="login-form-heading text-center">
                                    <img class="p-4" src="~/Public/images/logo/logo_orange.svg" width="60%" />
                                    <h1 class="text-uppercase text-center">welcome</h1>
                                    <p class="text-uppercase text-center">
                                        if you want to
                                        <span>book</span> a tour use
                                        <t>traveler login</t>
                                    </p>
                                </div>
                                <form action="@Url.Action("login", "home")" method="post" id="guide-login-form" novalidate>
                                    <input type="number" name="accounttypeid" value="1" hidden />
                                    <div class="form-group">
                                        <label for="guide-email">E-mail</label>
                                        <input class="form-control" type="email" name="email" id="guide-email" data-validation="required email">
                                    </div>
                                    <div class="form-group">
                                        <label for="guide-password">Password</label>
                                        <input class="form-control" type="password" name="password" id="guide-password" data-validation="length" data-validation-length="min8">
                                    </div>
                                    <div class="form-buttons my-4 text-center">
                                        <button type="submit" class="btn btn-success btn-block text-uppercase">Log in</button>
                                        <button type="button" class="btn btn-outline-secondary btn-block go-to-signup">Don't have an account? Sign Up</button>
                                        <a href="#" class="btn btn-link pass-recovery">Forgot Password?</a>
                                    </div>
                                </form>
                            </div>
                            <!-- Guide Login Tab End -->
                        </div>
                    </div>
                    <i class="fas fa-times" id="form-modal-close" data-dismiss="modal"></i>
                </div>
            </div>
        </div>
        @if (Session["LogInSuccess"] != null)
        {
            <span id="login-alert-success" hidden></span>
            Session["LogInSuccess"] = null;
        }
        else if (Session["LogInError"] != null)
        {
            <span id="login-alert-error" hidden></span>
            Session["LogInError"] = null;
        }
    </div>

    <!-- Password Recovery Form Modal -->
    <div class="pass-recovery-form-modal-wrapper">
        <div class="modal" id="pass-recovery-form-modal" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-body" id="pass-recovery-form-body">
                        <div class="login-form-heading">
                            <h2 class="text-capitalize text-center">tour master</h2>
                            <h1 class="text-uppercase text-center">welcome</h1>
                            <p class="text-uppercase text-center">
                                enter your
                                <span>email</span> to recover your password
                            </p>
                        </div>
                        <form action="" method="post" id="pass-recovery-form" novalidate>
                            <div class="form-group">
                                <label for="recover-email">E-mail</label>
                                <input class="form-control" type="email" name="email" id="recover-email" data-validation="required email">
                            </div>
                            <div class="form-buttons my-4 text-center">
                                <button type="submit" class="btn btn-success btn-block text-uppercase">Send recovery email</button>
                                <button type="button" class="btn btn-outline-secondary btn-block go-to-signup">Don't have an account? Sign Up</button>
                            </div>
                        </form>
                    </div>
                    <i class="fas fa-times" id="form-modal-close" data-dismiss="modal"></i>
                </div>
            </div>
        </div>
    </div>

    <!-- Tours Modal Section -->
    <div class="tours-modal-wrapper">
        <div class="row">
            <div class="col-12">
                <div class="modal" id="tours-modal" tabindex="-1" role="dialog" data-backdrop="false">
                    <section id="tours">
                        <!-- Section Heading -->
                        <div class="section-heading-wrapper text-center">
                            <h1 class="section-title text-uppercase">Tours Available</h1>
                            <h4 class="section-subtitle text-uppercase">Here is our tours</h4>
                        </div>
                        <!-- Section Body -->
                        <div class="container">
                            <!-- Filter Buttons -->
                            <div class="tours-filters-wrapper text-center">
                                <div class="btn-group btn-group-lg flex-wrap filter-button-group justify-content-center">
                                    <button class="btn btn-info active" data-filter="*" type="button">All</button>
                                    @foreach (Category cat in Model.Categories)
                                    {
                                        <button class="btn btn-info" data-filter=".@cat.CategoryName.ToLower()" type="button">@cat.CategoryName</button>

                                    }
                                </div>
                            </div>
                            <!-- Tours Search -->
                            <div class="tours-search-wrapper">
                                <div class="accordion" id="search-accordion">
                                    <div class="card">
                                        <div class="card-header" id="search-trigger">
                                            <h5 class="mb-0 text-center">
                                                <button class="btn btn-info text-uppercase" type="button" data-toggle="collapse" data-target="#search-wrapper">
                                                    Search for specific tour
                                                </button>
                                            </h5>
                                        </div>
                                        <div id="search-wrapper" class="collapse" data-parent="search-accordion">
                                            <div class="card-body">
                                                <form action="" method="get" id="tours-modal-search-form">
                                                    <div class="form-group">
                                                        <label for="search">Search</label>
                                                        <input class="form-control search-input" type="text" id="search" name="search">
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label for="min-rating">
                                                                    Min. Rating
                                                                    <span>0</span>
                                                                </label>
                                                                <input class="form-control-range" type="range" name="min-rating" id="min-rating" min="0" max="5" step="1" value="0">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label for="min-price">
                                                                    Min. Price
                                                                    <span>@Model.Tours.OrderBy(t => t.Price).FirstOrDefault().Price.ToString("#.##")</span>
                                                                </label>
                                                                <input class="form-control-range" type="range" name="min-price" id="min-price" min="@Model.Tours.OrderBy(t=>t.Price).FirstOrDefault().Price.ToString("#.##")" max="@Model.Tours.OrderByDescending(t=>t.Price).FirstOrDefault().Price.ToString("#.##")" step="1" value="@Model.Tours.OrderBy(t=>t.Price).FirstOrDefault().Price.ToString("#.##")">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label for="max-price">
                                                                    Max. Price
                                                                    <span>@Model.Tours.OrderByDescending(t => t.Price).FirstOrDefault().Price.ToString("#.##")</span>
                                                                </label>
                                                                <input class="form-control-range" type="range" name="max-price" id="max-price" min="100" max="@Model.Tours.OrderByDescending(t=>t.Price).FirstOrDefault().Price.ToString("#.##")" step="1" value="@Model.Tours.OrderByDescending(t=>t.Price).FirstOrDefault().Price.ToString("#.##")">
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3">
                                                            <div class="form-group">
                                                                <label for="duration-days">
                                                                    Days
                                                                    <span>1</span>
                                                                </label>
                                                                <input class="form-control-range" type="range" name="duration-days" id="duration-type" min="1" max="@Model.Tours.OrderByDescending(t=>t.DurationTypeId).OrderByDescending(t=>t.Duration).FirstOrDefault().Duration" step="1" value="1">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="form-button text-center">
                                                        <button class="btn btn-secondary text-uppercase" type="submit">Search</button>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Filter Items -->
                            <div class="tours-grid-wrapper mt-4">
                                <div class="tours-grid">
                                    <!-- Gutter Sizer -->
                                    <div class="gutter-sizer"></div>
                                    @foreach (Tour tour in Model.Tours.Where(t => t.Status == 1 && t.Approved == 1).OrderByDescending(t => t.PostedDate).Take(6))
                                    {
                                        string[] tourCats = tour.Category.Split(',');
                                        string tourCatsAll = "";
                                        foreach (string category in tourCats)
                                        {
                                            tourCatsAll += " " + category;
                                        }
                                        <!-- Grid Item Start -->
                                        <div class="grid-item @tourCatsAll" style="background-image: url('@tour.TourImage.ImageURL')">
                                            <div class="info-overlay">
                                                <div class="tour-heading">
                                                    @{
                                                        string tourTitle = "";
                                                        if (tour.FromId == tour.DestinationId)
                                                        {
                                                            tourTitle = tour.City.CityName;
                                                        }
                                                        else
                                                        {
                                                            tourTitle = tour.City.CityName + " - " + tour.City1.CityName;
                                                        }
                                                    }
                                                    <h3 class="tour-title">@tourTitle Tour</h3>
                                                    <p class="duration" title="Duration">
                                                        <i class="fal fa-clock"></i>@tour.Duration @tour.DurationType.Type
                                                    </p>
                                                    <p class="price" title="Price">
                                                        <i class="fal fa-money-bill"></i>@tour.Price.ToString("#.##") @tour.Currency.CurrencyName
                                                    </p>
                                                    <button class="btn btn-primary text-uppercase my-3 tour-details" data-tour-id="@tour.Id">Details</button>
                                                </div>
                                                <div class="tour-guide">
                                                    @{
                                                        User guide = tour.User;
                                                    }
                                                    <img src="@guide.ProfileImage">
                                                    <ul class="list-unstyled">
                                                        <li>@guide.Fullname</li>
                                                        <li>
                                                            <select class="tour-guide-rating">
                                                                <option value="1" @(guide.OverallRating <= 1 ? "selected" : "")>1</option>
                                                                <option value="2" @(guide.OverallRating == 2 ? "selected" : "")>2</option>
                                                                <option value="3" @(guide.OverallRating == 3 ? "selected" : "")>3</option>
                                                                <option value="4" @(guide.OverallRating == 4 ? "selected" : "")>4</option>
                                                                <option value="5" @(guide.OverallRating == 5 ? "selected" : "")>5</option>
                                                            </select>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                            <div class="info-overlay-bottom">
                                                <p>
                                                    @tourTitle Tour
                                                    <span>@tour.Price.ToString("#.##") @tour.Currency.CurrencyName</span>
                                                </p>
                                            </div>
                                        </div>
                                        <!-- Grid Item End -->
                                    }
                                </div>
                                <div class="tours-load-button text-center mb-5">
                                    <button class="btn btn-secondary text-capitalize" id="load-more-tours">Load More</button>
                                </div>
                            </div>
                        </div>
                    </section>
                    <i class="fas fa-times" id="tours-close"></i>
                </div>
            </div>
        </div>
    </div>

    <!-- Tour View Modal -->
    <div class="tour-view-modal-wrapper">
        <div class="row">
            <div class="col-12">
                <div class="modal" id="tour-view-modal" tabindex="-1" role="dialog" data-backdrop="false">
                    <section id="tour-view">
                        <!-- Tour Heading -->
                        <div class="section-heading-wrapper text-center">
                            <h1 class="section-title text-uppercase"></h1>
                        </div>
                        <!-- Tour Body Start -->
                        <!-- Tour View Slider Start -->
