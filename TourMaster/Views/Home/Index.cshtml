        <!-- Section Body -->
        <div class="section-body-wrapper">
            <div class="container">
                <div class="row">
                    @{
                        foreach (User bestguide in Model.BestGuides)
                        {
                            <!-- Best Guide Item Start -->
                            <div class="col-md-4">
                                <div class="card">
                                    <div class="card-heading">
                                        <img class="card-img-top" src="@bestguide.ProfileImage">
                                        <div class="overlay text-center">
                                            <a class="best-guide-profile text-uppercase btn btn-outline-success" data-guide-id="@bestguide.Id">Go to profile</a>
                                        </div>
                                    </div>
                                    <div class="card-body text-center">
                                        <h4 class="card-title text-uppercase">@bestguide.Fullname</h4>
                                        <select class="best-guide-rating">
                                            <option value="1" @(bestguide.OverallRating <= 1 ? "selected" : "")>1</option>
                                            <option value="2" @(bestguide.OverallRating == 2 ? "selected" : "")>2</option>
                                            <option value="3" @(bestguide.OverallRating == 3 ? "selected" : "")>3</option>
                                            <option value="4" @(bestguide.OverallRating == 4 ? "selected" : "")>4</option>
                                            <option value="5" @(bestguide.OverallRating == 5 ? "selected" : "")>5</option>
                                        </select>
                                    </div>
                                    <ul class="list-group list-group-flush">
                                        <li class="list-group-item text-capitalize">
